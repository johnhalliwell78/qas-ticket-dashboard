    "nodemon": "^1.12.1",
    "run-sequence": "^1.2.2",
    "sinon": "^2.1.0",
    "supertest": "^2.0.1",
    "ts-node": "^3.0.2",
    "tslint": "^5.8.0",
    "typescript": "^2.5.3"
  }
}
