{
  "name": "qas-ticket-dashboard",
  "version": "1.0.0",
  "scripts": {
    "clean": "./node_modules/.bin/gulp clean",
    "build:client": "cd ../client && npm run build && cd ../server",
    "build:client:prod": "cd ../client && npm run build:prod && cd ../server",
    "build:server": "cross-env NODE_ENV=development ./node_modules/.bin/gulp build",
    "build": "npm run build:client && npm run build:server",
    "tslint": "./node_modules/.bin/gulp tslint",
    "poststart": "node ./node_modules/.bin/pm2 logs",
    "stop": "node ./node_modules/.bin/pm2 stop ./pm2.json",
    "start": "node ./node_modules/.bin/pm2 start ./pm2.json",
    "watch": "cross-env NODE_ENV=development ./node_modules/.bin/gulp start",
    "package:ec2": "npm run build:client:prod && cross-env NODE_ENV=production ./node_modules/.bin/gulp package-ec2"
  },
  "repository": {
    "type": "git"
  },
  "dependencies": {
    "archiver": "^2.1.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.3",
    "dotenv": "^5.0.1",
    "express": "^4.16.2",
    "fs-extra": "^4.0.1",
    "i18n": "^0.8.3",
    "jsonwebtoken": "^7.4.3",
    "lodash": "^4.17.4",
    "logs-to-sumologic": "^1.1.0",
    "moment": "^2.19.1",
    "mongodb": "^2.2.31",
    "mongoose": "^4.12.3",
    "mongration": "^1.0.0",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "node-dir": "^0.1.17",
    "node-fetch": "^2.1.1",
    "node-xlsx": "^0.7.4",
    "optimist": "^0.6.1",
    "pm2": "^2.7.2",
    "pug": "^2.0.0-rc.3",
    "request": "^2.83.0",
    "serve-favicon": "^2.4.5",
    "swagger-jsdoc": "^1.9.1",
    "swagger-ui": "^2.2.10",
    "winston": "^2.4.0",
    "winston-daily-rotate-file": "^1.7.2",
    "yaml-fragment": "^1.0.1"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.2",
    "@types/body-parser": "1.16.3",
    "@types/chai": "^3.4.35",
    "@types/chai-http": "0.0.30",
    "@types/cookie-parser": "^1.3.30",
    "@types/errorhandler": "0.0.30",
    "@types/express": "^4.0.36",
    "@types/method-override": "0.0.29",
    "@types/mocha": "^2.2.33",
    "@types/mongoose": "^4.7.24",
    "@types/morgan": "^1.7.33",
    "@types/multer": "^1.3.3",
    "@types/node": "^8.0.46",
    "@types/winston": "^2.3.6",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "concurrently": "^3.0.0",
    "cross-env": "^5.1.0",
    "del": "^2.2.2",
    "factory-girl": "^4.0.0-beta.7",
    "faker": "^3.1.0",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-filter": "^5.0.0",
    "gulp-mocha": "^3.0.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-tslint": "^8.1.1",
    "gulp-typescript": "^3.2.1",
    "merge-stream": "^1.0.1",
